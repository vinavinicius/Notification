{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "UEAT.Notification": "Debug"
    }
  },
  "DetailedErrors": true,
  "TwilioConfigurations": {
    "AccountKey": "your-twilio-account-key",
    "AccountName": "your-twilio-account-name",
    "NumberFrom": "+1234567890"
  },
